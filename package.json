{
	"name": "inio-server",
	"version": "1.0.0",
	"engines": {
		"node": "14.8.0"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/jkasztur/inio-server.git"
	},
	"scripts": {
		"start": "run-p db:migrate start:prod",
		"start:prod": "node bin/server.js",
		"watch": "run-p db:migrate start:local",
		"start:local": "ts-node-dev --prefer-ts --respawn --files ./bin/server.js",
		"postinstall": "tsc",
		"clean": "find src/ -type f -iname '*.js' -delete",
		"db:migrate": "sequelize-cli db:migrate"
	},
	"author": "Jan Kasztura",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/jkasztur/inio-server/issues"
	},
	"homepage": "https://github.com/jkasztur/inio-server#readme",
	"devDependencies": {
		"@types/koa": "^2.13.4",
		"@types/koa-router": "^7.4.4",
		"@types/node": "^16.7.10",
		"ts-node-dev": "^1.1.8",
		"typescript": "^4.4.2"
	},
	"dependencies": {
		"@koa/cors": "^3.1.0",
		"argon2": "^0.28.2",
		"axios": "^0.21.1",
		"dotenv": "^10.0.0",
		"http-status-codes": "^2.1.4",
		"joi": "^17.4.2",
		"koa": "^2.13.1",
		"koa-body": "^4.2.0",
		"koa-router": "^10.1.1",
		"node-injectable": "^3.2.0",
		"npm-run-all": "^4.1.5",
		"pg": "^8.7.1",
		"pg-hstore": "^2.3.3",
		"reflect-metadata": "^0.1.13",
		"sequelize": "^5.21.12",
		"sequelize-cli": "^6.2.0",
		"sequelize-typescript": "^1.1.0",
		"source-map-support": "^0.5.19"
	}
}